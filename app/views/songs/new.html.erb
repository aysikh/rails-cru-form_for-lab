<h1> Add A New Song! </h1>


<%= form_for(@songs) do |f| %>
  <%= f.label "Name of Song:" %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :artist_id %>
  <%= f.text_field :artist_id %>
  <br><br>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id %>
  <br><br>

  <%= f.submit 'ADD!' %>
<% end %>